<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<Appenders>
		<Console name="Console1">
			<PatternLayout
				pattern="%d{dd-MMM-yyyy HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="All">
			<AppenderRef ref="Console1">

			</AppenderRef>
		</Root>
	</Loggers>
</configuration>